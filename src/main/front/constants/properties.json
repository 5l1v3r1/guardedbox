{
    "general": {
        "emailMaxLength": 254,
        "emailPattern": ".+@.+\\..{2,}"
    },
    "registration": {
        "registrationTokenPattern": "^[a-zA-Z0-9]{86}$",
        "passwordMinLength": 20,
        "passwordMinStrength": 100
    },
    "secrets": {
        "secretNameMaxLength": 100,
        "secretValueMaxLength": 4000,
        "secretStrengthMaxLength": 40,
        "randomSecretCharset": "!\"#$%&'()*+,-./0123456789:;<=>?@ABCDEFGHIJKLMNOPQRSTUVWXYZ[\\]^_`abcdefghijklmnopqrstuvwxyz{|}~",
        "showSecretTime": 2500
    },
    "groups": {
        "groupNameMaxLength": 100
    },
    "cryptography": {
        "length": 32,
        "ecc": {
            "ecdhCurve": "curve25519",
            "eddsaCurve": "ed25519",
            "pbkdf2Hash": "sha256",
            "pbkdf2Iterations": 10000
        },
        "aes": {
            "mode": "aes-256-gcm",
            "ivLength": 12
        }
    },
    "keys": {
        "encryptionPublicKeyMaxLength": 44,
        "encryptionPublicKeyPattern": "[a-zA-Z0-9+\/=]{44}"
    }
}
